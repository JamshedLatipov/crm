# Example env file for local development. Do NOT commit real secrets to VCS.
# Copy to .env and fill values for your local environment.

# Postgres
POSTGRES_USER=postgres
POSTGRES_PASSWORD=changeme
POSTGRES_DB=crm

# Backend DB connection (back service)
DB_HOST=postgres
DB_PORT=5432
DB_USER=postgres
DB_PASS=changeme
DB_NAME=crm

# RabbitMQ
RMQ_URL=amqp://rabbitmq:5672
RMQ_QUEUE=crm_queue

# Redis
REDIS_URL=redis://redis:6379

# Asterisk ARI
ARI_HOST=asterisk
ARI_PORT=8089
ARI_USER=ariuser
ARI_PASSWORD=changeme
ARI_APP=crm-app

# JWT secret (backend)
JWT_SECRET=please_change_me

  "scripts": {
    "start:back": "nx serve back --host 0.0.0.0 --watch",
    "start:front": "nx serve front --host 0.0.0.0",
    "start:identity": "nx serve identity-service --watch",
    "start:contact": "nx serve contact-service --watch",
    "start:lead": "nx serve lead-service --watch",
    "start:deal": "nx serve deal-service --watch",
    "start:task": "nx serve task-service --watch",
    "start:notification": "nx serve notification-service --watch",
    "start:telephony": "nx serve telephony-service --watch",
    "start:analytics": "nx serve analytics-service --watch",
    "start:campaign": "nx serve campaign-service --watch",
    "start:audit": "nx serve audit-service --watch",
    "start:gateway": "nx serve api-gateway --watch",
    "build": "npm run build:back && npm run build:front",
    "build:back": "nx build back",
    "build:front": "nx build front",
    "build:identity": "nx build identity-service",
    "build:contact": "nx build contact-service",
    "build:lead": "nx build lead-service",
    "build:deal": "nx build deal-service",
    "build:task": "nx build task-service",
    "build:notification": "nx build notification-service",
    "build:telephony": "nx build telephony-service",
    "build:analytics": "nx build analytics-service",
    "build:campaign": "nx build campaign-service",
    "build:audit": "nx build audit-service",
    "build:gateway": "nx build api-gateway",
    "build:contracts": "nx build @crm/contracts",
    "build:microservices": "npm run build:contracts && npm run build:identity && npm run build:contact && npm run build:lead && npm run build:deal && npm run build:task && npm run build:notification && npm run build:telephony && npm run build:analytics && npm run build:campaign && npm run build:audit && npm run build:gateway",
    "build:all": "npm run build:microservices && npm run build:back && npm run build:front",
    "test:back": "nx test back",
    "test:front": "nx test front",
    "test:identity": "nx test identity-service",
    "test:contact": "nx test contact-service",
    "test:lead": "nx test lead-service",
    "test:deal": "nx test deal-service",
    "test:task": "nx test task-service",
    "test:notification": "nx test notification-service",
    "test:telephony": "nx test telephony-service",
    "test:analytics": "nx test analytics-service",
    "test:campaign": "nx test campaign-service",
    "test:audit": "nx test audit-service",
    "test:gateway": "nx test api-gateway",
    "test:all": "nx run-many --target=test --all",
    "lint:all": "nx run-many --target=lint --all",
    "start:services": "docker-compose up -d turn rabbitmq redis postgres asterisk",
    "start:microservices": "docker-compose up -d identity-service api-gateway audit-service",
    "start:all": "npm run start:services && npm run start:microservices",
    "stop:services": "docker-compose down",
    "logs:services": "docker-compose logs -f",
    "dev": "nx run-many --target=serve --projects=back,front --parallel=2",
    "dev:back": "npm run start:back",
    "dev:front": "npm run start:front",
    "dev:ms": "nx run-many --target=serve --projects=identity-service,api-gateway,audit-service --parallel=3",
    "dev:ms:all": "nx run-many --target=serve --projects=identity-service,lead-service,deal-service,contact-service,task-service,notification-service,telephony-service,analytics-service,campaign-service,audit-service,api-gateway --parallel=11",
    "dev:full": "nx run-many --target=serve --projects=back,front,identity-service,api-gateway --parallel=4",
    "infra:up": "docker-compose up -d postgres redis rabbitmq",
    "infra:down": "docker-compose down",
    "infra:logs": "docker-compose logs -f postgres redis rabbitmq"
  },
